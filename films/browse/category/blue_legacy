<script type="text/javascript">
  SNAG.loadOnReady.add(function() {
    var js_file = "http://cdn.snagfilms.com/resource/assets/scripts/jquery.autocomplete.c95ce2984cbf94aba9816e5891ca913d.js"; 
    $.getScript(js_file, function(){
      var options, a;
      options = { serviceUrl:'/apis/titlesearch.json',
	  			  minChars:3,
	  			  deferRequestBy: 100, //miliseconds
	  			  width:375,
	  			  onSelect: function(value, data){window.location.href = data;}
	  			  };
      a = $('#query').autocomplete(options);
      a = $('#fquery').autocomplete(options);
      a.enable();
    });
	}, 'low');
</script>

