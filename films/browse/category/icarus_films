  <script type="text/javascript">     

    $(document).ready(function() {        
      SNAG.loadOnReady.execute('high');      
      if(SNAG.deferLoad) {      
        setTimeout(function(){
          if(!SNAG.loadFinished) {
            SNAG.runWhenReady();
