    <script type="text/javascript">

  SNAG.loadOnReady.add(function() {
    Snag.Tracking.GA.trackEvent("film page", "login-comment-prompt-begin");
    
    
    SNAG.onFBInit.push(function(fb_object) {
      // unsubscribe regular login
      fb_object.Event.unsubscribe("auth.authResponseChange", Snag.fb.authResponseChange);
  
      fb_object.Event.subscribe("auth.authResponseChange", function(response) {
        if (response.status == "connected") {
          Snag.fb.authd = true;
          Snag.Tracking.GA.trackEvent("film page", "login-comment-facebook");
          $.ajax({
              type: "get",
              url: "/fbLoginNoRedirect.jsp?token="
                  + encodeURIComponent(response.authResponse.accessToken)
                  + "&expires=" + response.authResponse.expiresIn
                  + "&uid=" + response.authResponse.userID,
              success: function(data) {
              },
          });
          Snag.fb.reInit(response);
        } else if (response.status == "unknown")  {
          
          document.cookie = 'fbsr_227991737252697 = ""; expired=0; path=/; domain=.snagfilms.com';
          Snag.loginPrompt.hide();
        }
      });
    });
    
    $(".nl-content .dontAskAgain").click(function(event){
        Snag.Tracking.GA.trackEvent("film page", "login-comment-dontAskAgain");
        event.preventDefault();
        $('.commentLoginPrompt').hide();
        Snag.loginPrompt.dontAskAgain();
    });
    $(".nl-content .cancel").click(function(event){
        Snag.Tracking.GA.trackEvent("film page", "login-comment-skip");
        event.preventDefault();
        $('.commentLoginPrompt').hide();
        Snag.loginPrompt.isSkipOnComment = true;
    });
    
    window.loginEmailSubmit = function() {
        if(!$("#icpsignup2179 [name='fields_email']").val()) {
            return false;
        }
        $.get("/login.jsp", $("#icpsignup2179").serialize(), function(data) {
            $(".commentLoginPrompt").children().remove();
            $(".commentLoginPrompt").html(data);
        });
        return false;
    }    
  },'low');   
</script></div>
