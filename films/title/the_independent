<script type="text/javascript">

    var companionHideTimer;
    function vastCompanions(ads){	
        console.log("Companion Ad Present");	
        if(ads[0].htmlResource)
        {
            loadRBs('companion', ads[0].width, ads[0].height, ads[0].htmlResource,  ads[0].companionClickThrough)
        }
	
        for (var i=0; i<ads.length; i++)
        {
            for (var name in ads[i])
            {
                //console.log(name)
            }
            console.log(ads[i].staticResource)
        }
    }

    function loadRBs(s,w,h,adTag, clickThrough)
    {
        console.log("Companion Ad Placed");
        try {
            if ($('.companion').length > 0) {
                $('.films-wrap').hide();
                $('.companion').html("<a href=\""
                    + clickThrough
                    + "\" target=\"_blank\">"
                    + adTag
                    + "</a>").show();
                    
                companionHideTimer=setInterval(function(){hideCompanion()},30000);
            }
        } catch(err) {
            console.log("Companion Ad Placement Error");
        }
    }

    function hideCompanion(){
        console.log("Hiding Companion Ad");
        clearInterval(companionHideTimer);
        if ($('.companion').length > 0) {
            $('.companion').hide();
            $('.films-wrap').show();
        }
    }

    </script><div class="share-film">
