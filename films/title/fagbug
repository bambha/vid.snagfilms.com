<script type="text/javascript">
  SNAG.loadOnReady.add(function(){   
  	 $('#share-film-twitter').html('<a href="https://twitter.com/share" class="twitter-share-button" data-text="Watching ' + $('meta[property="og:title"]').attr('content') + ' @SnagFilms | Watch the Movie Free Online">Tweet</a></li>');
		(function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}})(document,"script","twitter-wjs");
      
    $(".auto-sharing :checkbox").change(function(event) {
        if(!Snag.fb.hasPublishStreamPermission) {
            Snag.fb.getPublishStreamPermission(true, function() {
                if(!Snag.fb.hasPublishStreamPermission) {
                    return;
                }                
                Snag.fb.enableAutoSharing();
            });
        } else {
            Snag.fb.toggleAutoSharing();
        }
    });
  }, 'low');  
</script></div> <!-- end video lead module -->
